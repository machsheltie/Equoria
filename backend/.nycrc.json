{
  "all": true,
  "check-coverage": true,
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov",
    "json",
    "json-summary",
    "cobertura"
  ],
  "include": [
    "**/*.mjs",
    "**/*.js"
  ],
  "exclude": [
    "coverage/**",
    "node_modules/**",
    "tests/**",
    "test/**",
    "**/*.test.mjs",
    "**/*.test.js",
    "**/*.spec.mjs",
    "**/*.spec.js",
    "jest.config.js",
    "jest.setup.mjs",
    "eslint.config.mjs",
    "babel.config.js",
    "server.mjs",
    "scripts/migrate.mjs",
    "seed/**",
    "migrations/**",
    "dist/**",
    "build/**",
    "docs/**",
    "examples/**"
  ],
  "branches": 80,
  "lines": 85,
  "functions": 80,
  "statements": 85,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  },
  "report-dir": "./coverage",
  "temp-dir": "./coverage/.nyc_output",
  "cache": true,
  "cache-dir": "./coverage/.nyc_cache",
  "instrument": true,
  "source-map": true,
  "produce-source-map": true,
  "skip-full": false,
  "skip-empty": false,
  "clean": true
}
